@model List<Vidly.Models.Movie>
@{
    ViewBag.Title = "Movies";
}

<h2>Movies</h2>

<table class="table">
    <thead>
        <tr class="table-secondary">
            <th scope="col">Id</th>
            <th scope="col">Title</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>@movie.Id</td>
                <td>@movie.Name</td>
            </tr>
        }
    </tbody>
</table>

